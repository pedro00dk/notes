{
    "compilerOptions": {
        "allowJs": true,
        "isolatedModules": true,
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmit": true,
        "outDir": "./dist/",
        "resolveJsonModule": true,
        "strict": true,
        "target": "ESNext",
        "types": ["vite/client"]
    }
}
